{
  "routes": [
    {
      "route": "/login",
      "rewrite": "/.auth/login/google"
    },
    {
      "route": "/logout",
      "rewrite": "/.auth/logout"
    },
    {
      "route": "/blog/last",
      "rewrite": "/blog/last/index.html"
    },
    {
      "route": "/blog/*",
      "rewrite": "/blog/[bid]/index.html"
    }
  ],
  "navigationFallback": {
    "rewrite": "index.html",
    "exclude": ["*.{png,jpg,gif,svg}", "*.css"]
  }
}